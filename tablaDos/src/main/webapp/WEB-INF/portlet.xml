	</portlet>
	<portlet>
		<portlet-name>tabla-portlet2</portlet-name>
		<display-name>Tabla Portlet2</display-name>
		<portlet-class>com.tabla.TablaPortlet2</portlet-class>
		<init-param>
			<name>view-template</name>
			<value>/html/tablaportlet2/view.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
